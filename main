public class Main {
    public static void main(String[] args) {
        // Создаём пользователя через Builder
        User user = new User.Builder("Abylai", "Urgen")
                .age(25)
                .email("abylaiurgen@gmail.com")
                .phone("+77052916016")
                .build();

        System.out.println(user);

        // Можно создать и с минимальными данными
        User simpleUser = new User.Builder("Abylai", "Urgen").build();
        System.out.println(simpleUser);
    }
}
