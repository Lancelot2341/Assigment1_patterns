public class Main {
    public static void main(String[] args) {
        // Создаём пользователя через Builder
        User user = new User.Builder("John", "Doe")
                .age(25)
                .email("john.doe@example.com")
                .phone("+123456789")
                .build();

        System.out.println(user);

        // Можно создать и с минимальными данными
        User simpleUser = new User.Builder("Alice", "Smith").build();
        System.out.println(simpleUser);
    }
}
